<head>
  <style type="text/css">
    .turbolinks-progress-bar {
      position: fixed;
      display: block;
      top: 0;
      left: 0;
      height: 3px;
      background: #0076ff;
      z-index: 9999;
      transition: width 300ms ease-out, opacity 150ms 150ms ease-in;
      transform: translate3d(0, 0, 0);}
  </style>
  <link rel="stylesheet" media="all" href="/assets/application.css" data-turbolinks-track="reload">
  <title>WebCampBackendNew</title>
</head>

<body>
  <% if flash[:notice] %>
  <div class="book book-primary" role="book">
    <%= notice %>
  </div>
<% end %>
  <p>
    <strong>Title:</strong>
    <%= @book.title %>
  </p>
  <p>
    <strong>Body:</strong>
    <%= @book.body %>
  </p>
  <%= link_to "edit", "/books/#{@book.id}/edit" %>
   |
  <a class="back" href="/books">Back</a>
</body>